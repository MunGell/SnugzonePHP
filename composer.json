{
    "name": "mungell/snugzone-api",
    "description": "SnugZone API Client",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Shmavon Gazanchyan",
            "email": "sgazanchyan@gmail.com"
        }
    ],
    "require": {
        "psr/http-message": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug": "^1.0",
        "php-http/message-factory": "^1.0",
        "php-http/discovery": "^1.0",
        "nesbot/carbon": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "php-http/mock-client": "^1.0",
        "guzzlehttp/psr7": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Snugzone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snugzone\\Tests\\": "tests/"
        }
    },
    "scripts": {
